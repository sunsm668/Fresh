style lang=\"scss\" scoped>\r\n@import '../../style/viriables.scss';\r\n.wrapper{\r\n    padding: 0 .18rem;\r\n}\r\n.search{\r\n    display: flex;\r\n    margin: .14rem 0 .04rem 0;\r\n    &__back{\r\n        line-height: .32rem;\r\n        font-size: .2rem;\r\n        padding: .03rem .16rem 0 0;\r\n        color: #B6B6B6;\r\n    }\r\n    &__content{\r\n        display: flex;\r\n        flex: 1;\r\n        line-height: .32rem;\r\n        border-radius: .16rem;\r\n        background: $search-bgColor;\r\n        &__icon{\r\n            width: .16rem;\r\n            height: .16rem;\r\n            padding: .03rem .16rem 0;\r\n            color: $search-fontbgColor;\r\n        }\r\n        &__input{\r\n            flex: 1;\r\n            padding-right: .2rem;\r\n            height: .32rem;\r\n            outline: none;\r\n            border: none;\r\n            background: none;\r\n            font-size: .14rem;\r\n            color: $content-fontcolor;\r\n            line-height: .32rem;\r\n        }\r\n    }\r\n}\r\n</style>\r\n \r\n"],"sourceRoot":""}]}