N;AAAA;AAJb,CA3Ca,EAiDb;AACE;AACAF,EAAAA,IAAI,EAAE,cAFR;AAGEC,EAAAA,IAAI,EAAE,WAHR;AAIEC,EAAAA,SAAS,EAAE;AAAA,WAAM;AAAO;AAAoC,kCAA3C,CAAN;AAAA;AAJb,CAjDa,EAuDb;AACE;AACAF,EAAAA,IAAI,EAAE,WAFR;AAGEC,EAAAA,IAAI,EAAE,UAHR;AAIEC,EAAAA,SAAS,EAAE;AAAA,WAAM;AAAO;AAAmC,iCAA1C,CAAN;AAAA,GAJb;AAKEC,EAAAA,WALF,uBAKcC,EALd,EAKkBC,IALlB,EAKwBC,IALxB,EAK6B;AAAA,wBACLC,YADK;AAAA,QACjBC,OADiB,iBACjBA,OADiB;AAEzBA,IAAAA,OAAO,GAAGF,IAAI,CAAC;AAAEL,MAAAA,IAAI,EAAG;AAAT,KAAD,CAAP,GAA2BK,IAAI,EAAtC;AACD;AARH,CAvDa,EAiEb;AACE;AACAN,EAAAA,IAAI,EAAE,QAFR;AAGEC,EAAAA,IAAI,EAAE,OAHR;AAIEC,EAAAA,SAAS,EAAE;AAAA,WAAM;AAAO;AAAgC,0BAAvC,CAAN;AAAA,GAJb;AAKEC,EAAAA,WALF,uBAKcC,EALd,EAKkBC,IALlB,EAKwBC,IALxB,EAK6B;AAAA,yBACLC,YADK;AAAA,QACjBC,OADiB,kBACjBA,OADiB;AAEzBA,IAAAA,OAAO,GAAGF,IAAI,CAAC;AAAEL,MAAAA,IAAI,EAAG;AAAT,KAAD,CAAP,GAA2BK,IAAI,EAAtC;AACD;AARH,CAjEa,CA2Eb;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AAlFa,CAAf;AAqFA,IAAMG,MAAM,GAAGZ,YAAY,CAAC;AAC1Ba,EAAAA,OAAO,EAAEZ,oBAAoB,EADH;AAE1BC,EAAAA,MAAM,EAANA;AAF0B,CAAD,CAA3B,C,CAIA;;AACAU,MAAM,CAACE,UAAP,CAAkB,UAACP,EAAD,EAAKC,IAAL,EAAWC,IAAX,EAAoB;AAAA,uBAChBC,YADgB;AAAA,MAC5BC,OAD4B,kBAC5BA,OAD4B;;AAEpC,MAAG,CAACA,OAAD,IAAaJ,EAAE,CAACH,IAAH,KAAY,OAAZ,IAAuBG,EAAE,CAACH,IAAH,KAAY,UAAnD,EACA;AACEK,IAAAA,IAAI,CAAC;AAAEL,MAAAA,IAAI,EAAG;AAAT,KAAD,CAAJ;AACD,GAHD,MAGK;AACHK,IAAAA,IAAI;AACL;AACF,CARD;AAUA,eAAeG,MAAf","sourcesContent":["import { createRouter, createWebHashHistory } from 'vue-router'\n\nconst routes = [\n  {\n    // 首页\n    path: '/',\n    name: 'Home',\n    component: () => import(/* webpackChunkName: \"Home\" */ '../views/Home/Home')\n  },\n  {\n    // 购物车页\n    path: '/shopcart',\n    name: 'ShopCart',\n    component: () => import(/* webpackChunkName: \"ShopCart\" */ '../views/ShopCart/ShopCart')\n  },\n  {\n    //附近店铺页\n    path: '/shop/:id',\n    name: 'Shop',\n    component: () => import(/* webpackChunkName: \"Shop\" */ '../views/Shop/Shop')\n  },\n  {\n    // 确认订单页\n    path: '/orderconfirmation/:id',\n    name: 'OrderConfirmation',\n    component: () => import(/* webpackChunkName: \"OrderConfirmation\" */ '../views/OrderConfirmation/OrderConfirmation')\n  },\n  {\n    // 订单页\n    path: '/orderlist',\n    name: 'OrderList',\n    component: () => import(/* webpackChunkName: \"OrderList\" */ '../views/OrderList/OrderList')\n  },\n  {\n    // 个人中心页\n    path: '/mine',\n    name: 'Mine',\n    component: () => import(/* webpackChunkName: \"Mine\" */ '../views/Mine/Mine')\n  },\n  {\n    // 我的地址\n    path: '/address',\n    name: 'Address',\n    component: () => import(/* webpackChunkName: \"Address\" */ '../views/Address/Address')\n  },\n  {\n    // 新建地址\n    path: '/newaddress',\n    name: 'newAddress',\n    component: () => import(/* webpackChunkName: \"newAddress\" */ '../views/newAddress/newAddress')\n  },\n  {\n    // 编辑地址\n    path: '/address/:id',\n    name: 'upAddress',\n    component: () => import(/* webpackChunkName: \"upAddress\" */ '../views/upAddress/upAddress')\n  },\n  {\n    //注册页\n    path: '/register',\n    name: 'Register',\n    component: () => import(/* webpackChunkName: \"Register\" */ '..//views/Register/Register'),\n    beforeEnter(to, from, next){\n      const { isLogin } = localStorage;\n      isLogin ? next({ name : 'Home'}): next();\n    }\n  },\n  {\n    //登陆页\n    path: '/login',\n    name: 'Login',\n    component: () => import(/* webpackChunkName: \"Login\" */ '../views/Login/Login'),\n    beforeEnter(to, from, next){\n      const { isLogin } = localStorage;\n      isLogin ? next({ name : 'Home'}): next();\n    }\n  },\n  // {\n  //   path: '/about',\n  //   name: 'About',\n  //   // route level code-splitting\n  //   // this generates a separate chunk (about.[hash].js) for this route\n  //   // which is lazy-loaded when the route is visited.\n  //   component: () => import(/* webpackChunkName: \"about\" */ '../views/About.vue')\n  // }\n]\n\nconst router = createRouter({\n  history: createWebHashHistory(),\n  routes\n})\n// 用户为登陆只允许访问登录页和注册页\nrouter.beforeEach((to, from, next) => {\n  const { isLogin } = localStorage;\n  if(!isLogin && (to.name !== \"Login\" && to.name !== \"Register\"))\n  {\n    next({ name : 'Login'});\n  }else{\n    next();\n  }\n})\n\nexport default router\n"]}]}